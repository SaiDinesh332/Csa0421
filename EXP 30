#include <stdio.h>
#include <pthread.h>

void* task() {
    printf("Thread executing\n");
    pthread_exit(NULL);
}

int main() {
    pthread_t t;
    pthread_create(&t, NULL, task, NULL);

    pthread_join(t, NULL);

    if (pthread_equal(pthread_self(), t))
        printf("Threads are equal\n");
    else
        printf("Threads are not equal\n");

    return 0;
}


OUTPUT:
Thread executing
Threads are not equal
