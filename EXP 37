#include <stdio.h>
#include <stdlib.h>

int main() {
    int req[10], n, head, seek = 0;

    printf("Enter number of requests: ");
    scanf("%d", &n);

    printf("Enter request queue:\n");
    for (int i = 0; i < n; i++)
        scanf("%d", &req[i]);

    printf("Enter initial head position: ");
    scanf("%d", &head);

    for (int i = 0; i < n; i++) {
        seek += abs(req[i] - head);
        head = req[i];
    }

    printf("Total Seek Time = %d\n", seek);
    return 0;
}
  OUTPUT:
 Enter number of requests: 5
Enter request queue:
98 183 37 122 14
Enter initial head position: 50
Total Seek Time = 460

