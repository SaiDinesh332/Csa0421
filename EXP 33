#include <stdio.h>

int main() {
    int frames, pages, faults = 0;
    int frame[10], ref[20];

    printf("Enter number of frames: ");
    scanf("%d", &frames);

    printf("Enter number of pages: ");
    scanf("%d", &pages);

    printf("Enter reference string:\n");
    for (int i = 0; i < pages; i++)
        scanf("%d", &ref[i]);

    for (int i = 0; i < frames; i++)
        frame[i] = -1;

    for (int i = 0; i < pages; i++) {
        int found = 0;

        for (int j = 0; j < frames; j++)
            if (frame[j] == ref[i]) {
                found = 1;
                break;
            }

        if (!found) {
            int pos = -1, far = -1;

            for (int j = 0; j < frames; j++) {
                int k;
                for (k = i + 1; k < pages; k++)
                    if (frame[j] == ref[k])
                        break;

                if (k > far) {
                    far = k;
                    pos = j;
                }
            }

            frame[pos] = ref[i];
            faults++;
        }

        printf("After page %d: ", ref[i]);
        for (int j = 0; j < frames; j++)
            printf("%d ", frame[j]);
        printf("\n");
    }

    printf("Total Page Faults = %d\n", faults);
    return 0;
}


OUTPUT:
Enter number of frames: 3
Enter number of pages: 7
Enter reference string:
7 0 1 2 0 3 0
After page 7: 7 -1 -1
After page 0: 7 0 -1
After page 1: 7 0 1
After page 2: 2 0 1
After page 0: 2 0 1
After page 3: 3 0 1
After page 0: 3 0 1
Total Page Faults = 5
